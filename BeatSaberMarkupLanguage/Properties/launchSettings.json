{
  "profiles": {
    "Beat Saber": {
      "commandName": "Executable",
      "executablePath": "$(BeatSaberDir)\\Beat Saber.exe",
      "commandLineArgs": "--verbose --debug",
      "workingDirectory": "$(BeatSaberDir)",
      "environmentVariables": {
        "SteamAppId": "620980"
      }
    },
    "Beat Saber (FPFC)": {
      "commandName": "Executable",
      "executablePath": "$(BeatSaberDir)\\Beat Saber.exe",
      "commandLineArgs": "fpfc --verbose --debug",
      "workingDirectory": "$(BeatSaberDir)",
      "environmentVariables": {
        "SteamAppId": "620980"
      }
    },
    "Beat Saber (No VR)": {
      "commandName": "Executable",
      "executablePath": "$(BeatSaberDir)Beat Saber.exe",
      "commandLineArgs": "fpfc --verbose --debug -vrmode none",
      "workingDirectory": "$(BeatSaberDir)",
      "environmentVariables": {
        "SteamAppId": "620980"
      }
    },
    "Beat Saber (generate docs data)": {
      "commandName": "Executable",
      "executablePath": "$(BeatSaberDir)\\Beat Saber.exe",
      "commandLineArgs": "fpfc --verbose --debug --bsml-generate-documentation",
      "workingDirectory": "$(BeatSaberDir)",
      "environmentVariables": {
        "SteamAppId": "620980"
      }
    }
  }
}